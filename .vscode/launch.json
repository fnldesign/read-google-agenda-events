    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",            
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Current File with Output File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["-o test-default.csv"],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Current File with DateParams",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["-s 2022-08-01", "-e 2022-08-31"],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}